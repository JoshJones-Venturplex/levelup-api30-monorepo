{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "mat-swapi": {
      "root": "apps/mat-swapi/",
      "sourceRoot": "apps/mat-swapi/src",
      "projectType": "application",
      "prefix": "workspace",
      "schematics": {
        "@nrwl/schematics:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/mat-swapi",
            "index": "apps/mat-swapi/src/index.html",
            "main": "apps/mat-swapi/src/main.ts",
            "polyfills": "apps/mat-swapi/src/polyfills.ts",
            "tsConfig": "apps/mat-swapi/tsconfig.app.json",
            "assets": [
              "apps/mat-swapi/src/favicon.ico",
              "apps/mat-swapi/src/assets"
            ],
            "styles": [
              "apps/mat-swapi/src/styles.scss",
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/mat-swapi/src/environments/environment.ts",
                  "with": "apps/mat-swapi/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "mat-swapi:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "mat-swapi:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "mat-swapi:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/mat-swapi/tsconfig.app.json",
              "apps/mat-swapi/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/mat-swapi/jest.config.js",
            "tsConfig": "apps/mat-swapi/tsconfig.spec.json",
            "setupFile": "apps/mat-swapi/src/test-setup.ts"
          }
        }
      }
    },
    "mat-swapi-e2e": {
      "root": "apps/mat-swapi-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/mat-swapi-e2e/protractor.conf.js",
            "devServerTarget": "mat-swapi:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "mat-swapi:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/mat-swapi-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "material": {
      "root": "libs/material",
      "sourceRoot": "libs/material/src",
      "projectType": "library",
      "prefix": "workspace",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/material/tsconfig.lib.json",
              "libs/material/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/material/jest.config.js",
            "tsConfig": "libs/material/tsconfig.spec.json",
            "setupFile": "libs/material/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/schematics:component": {
          "styleext": "scss"
        }
      }
    },
    "common-data": {
      "root": "libs/common-data",
      "sourceRoot": "libs/common-data/src",
      "projectType": "library",
      "prefix": "workspace",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/common-data/tsconfig.lib.json",
              "libs/common-data/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/common-data/jest.config.js",
            "tsConfig": "libs/common-data/tsconfig.spec.json",
            "setupFile": "libs/common-data/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/schematics:component": {
          "styleext": "scss"
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/schematics",
    "packageManager": "yarn"
  },
  "schematics": {
    "@nrwl/schematics:application": {
      "style": "scss"
    },
    "@nrwl/schematics:library": {
      "style": "scss"
    }
  },
  "defaultProject": "mat-swapi"
}